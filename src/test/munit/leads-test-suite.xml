	<munit:config name="leads-test-suite.xml" />
	<munit:test name="post-leadsTest" doc:id="jbf8kr5k-js4o-baz8-0lj3-ij76167c1hc" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="xxl1imqv-heum-q4jd-xamv-4xru5cerlh" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="doc-3e19c4a8" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-leads" doc:id="j81etulz-uf58-n9xk-9j7i-zv7nf6kgbx" name="post-leads"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="wjy6xzdm-96gs-paan-lhb6-30n89b8qrjq" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success
}]'/>
		</munit:validation>
	</munit:test>
