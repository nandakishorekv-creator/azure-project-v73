		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="ewfz1gfo-0qs9-e16m-5gog-n0zc4jgj5y" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success
}]'/>
